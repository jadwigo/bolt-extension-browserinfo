{
    "name": "jadwigo/browserinfo",
    "description": "Display information about your browser",
    "type": "bolt-extension",
    "keywords": [
        "browscap", "browser", "information", "test"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Lodewijk evers",
            "email": "lodewijk@twokings.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Jadwigo\\BrowserInfo\\": "src"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Jadwigo\\BrowserInfo\\BrowserInfoExtension"
    }
}
